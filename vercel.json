{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "docs.ziggyplatform.com"
        }
      ],
      "destination": "https://docs.ziggyplatform.com/",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "docs.ziggyplatform.com"
        }
      ],
      "destination": "https://docs.ziggyplatform.com/:path*",
      "permanent": true
    }
  ]
}
